XXX_XML_XSD_TEST is

execute is
/*
https://community.oracle.com/message/14573711#14573711
не работает регистрация схемы в случае наличия ограничения превышающего 32767, например <xs:maxLength value="50000"/>
*/
	v_SchemaUrl			varchar2(100) := 'test.xsd';
	v_TestXSDinClob		clob;
	v_TestXMLinClob		clob;
	v_TestXML			XMLType;
	v_SchemaBasedXML	XMLType;

	v_BeginTime			integer;
	v_EndTime			integer;
	v_TempValidate		number;
begin
	unloadSchema(v_SchemaUrl);
		
	-- 1. инициализация XML и XSD; при правильной работе производится чтение XSD из справочника и XML с клиента
	v_BeginTime := utils.get_time;
	debug_pipe('----------------------------------------------------------------------', 0);
	debug_pipe('1. Инициализация', 0);
		-- тестирования XML и XSD из операции
		--initXSD(v_TestXSDinClob);
		--initXML(v_TestXMLinClob);
		-- тестирование XML и XSD из файла
		initXSDFromFile(v_TestXSDinClob, v_SchemaUrl);
		initXMLFromFile(v_TestXMLinClob);
	v_EndTime := utils.get_time;
	debug_pipe('Завершение инициализации XML и XSD: ' || (v_EndTime - v_BeginTime) / 100 || ' c.', 0);
	
	
	-- 2. проверка зарегистрирована ли схема, если её нет, то регистрация (проверяется по v_SchemaUrl)
	v_BeginTime := utils.get_time;
	debug_pipe('----------------------------------------------------------------------', 0);
	debug_pipe('2. Регистрация схемы XSD:', 0);
		loadSchema(v_SchemaUrl, v_TestXSDinClob);
	v_EndTime := utils.get_time;
	debug_pipe('Завершение регистрации схемы XSD: ' || (v_EndTime - v_BeginTime) / 100 || ' c.', 0);


	-- 3. разбор XML
	v_BeginTime := utils.get_time;
	debug_pipe('----------------------------------------------------------------------', 0);
	debug_pipe('3. Запуск разбора XML', 0);
		v_testXML := XMLType(v_TestXMLinClob);
	v_EndTime := utils.get_time;
	debug_pipe('Завершение разбора XML: ' || (v_EndTime - v_BeginTime) / 100 || ' c.', 0);


	-- 4. createSchemaBasedXML
	v_BeginTime := utils.get_time;
	debug_pipe('----------------------------------------------------------------------', 0);
	debug_pipe('4. createSchemaBasedXML', 0);
		v_SchemaBasedXML := v_testXML.createSchemaBasedXML(v_SchemaUrl);
	v_EndTime := utils.get_time;
	debug_pipe('Завершение createSchemaBasedXML: ' || (v_EndTime - v_BeginTime) / 100 || ' c.', 0);
			

	-- 5. простая проверка соответствия XSD
	v_BeginTime := utils.get_time;
	debug_pipe('----------------------------------------------------------------------', 0);
	debug_pipe('5. Запуск простой валидации (без вывода ошибок)', 0);
	begin
		v_TempValidate := XMLType.isSchemaValid(v_SchemaBasedXML);
	exception when others then
		debug_pipe('Exception: ' || (v_EndTime - v_BeginTime) / 100 || ' c.' || chr(10) || chr(13) || utils.error_stack, 0);
	end;
	v_EndTime := utils.get_time;
	debug_pipe('Завершение простой валидации: ' || (v_EndTime - v_BeginTime) / 100 || ' c. Результат: ' || v_TempValidate, 0);
	
		
	-- 6. продвинутая проверка соответствия XSD
	v_BeginTime := utils.get_time;
	debug_pipe('----------------------------------------------------------------------', 0);
	debug_pipe('6. Запуск продвинутой валидации (c выводом ошибок)', 0);
	begin
		XMLType.schemaValidate(v_SchemaBasedXML);
	exception when others then
		debug_pipe('Exception: ' || (v_EndTime - v_BeginTime) / 100 || ' c.' || chr(10) || chr(13) || utils.error_stack, 0);
	end;
	v_EndTime := utils.get_time;
	debug_pipe('Завершение продвинутой валидации: ' || (v_EndTime - v_BeginTime) / 100 || ' c.', 0);
	
	
	-- 7. удаление зарегистрированной схемы
	v_BeginTime := utils.get_time;
	debug_pipe('----------------------------------------------------------------------', 0);
	debug_pipe('7. Удаление зарегистрированной схемы', 0);
		unloadSchema(v_SchemaUrl);
	v_EndTime := utils.get_time;
	debug_pipe('Завершение удаления зарегистрированной схемы: ' || (v_EndTime - v_BeginTime) / 100 || ' c.', 0);

exception when others all then
	debug_pipe('X. Exception: ' || (v_EndTime - v_BeginTime) / 100 || ' c.' || chr(10) || chr(13) || utils.error_stack, 0);
	unloadSchema(v_SchemaUrl);
end;
end;
end;