@import_plsql(true)
@name('XXX. Пример операции проверки соответствия xml схеме xsd')
static method XXX_XML_XSD_TEST is
-- регистрация схемы
procedure loadSchema(p_SchemaURL in varchar2(100), p_TestXSDinClob in clob) is
	tempSchemaURL	varchar2(100);
begin
	-- https://docs.oracle.com/cd/B19306_01/appdev.102/b14258/d_xmlsch.htm#i1003177
	-- by default, all schemas are registered as local schemas, under /sys/schemas/<username>/...
	-- if a schema is registered as global, it is added under /sys/schemas/PUBLIC/...
	-- не зарегистрирована ли уже схема (для глобальной схемы использовать dba_xml_schemas)?
	begin
		select xml(xml.schema_url : schema_url) in user_xml_schemas%rowtype
		where xml.schema_url = p_SchemaURL
		into tempSchemaURL;	
	exception when no_data_found then
		/*
		gentypes  => false | gentables => false = 0.19 c
		gentypes  => true  | gentables => false = 4.72 c.
		gentypes  => false | gentables => true  = 16.08 c.
		gentypes  => true  | gentables => true  = 14.08 c.
		*/
		-- begin pl/sql
		DBMS_XMLSCHEMA.REGISTERSCHEMA(schemaurl => p_SchemaURL
									 ,schemadoc => p_TestXSDinClob
									 ,local		=> true							-- is this a local or global schema?
									 ,gentypes  => false						-- generate object types
									 ,genbean	=> false						-- no java beans
									 ,gentables => false						-- generate object tables
									 ,force     => false						-- if this parameter is set to TRUE, the schema registration will not raise errors
									 --csid      => nls_charset_id('UTF-8'));	-- identifies the character set of the input schema document
									 );
		-- end pl/sql
	end;
end;


-- удаление схемы
procedure unloadSchema(p_SchemaURL in varchar2(100)) is
begin
/*
DELETE_RESTRICT 		= 0.03 c.
DELETE_INVALIDATE		= 5.41 c.
DELETE_CASCADE 			= 5.08 c.
DELETE_CASCADE_FORCE	= 5.38 c.
*/
	-- begin pl/sql
	DBMS_XMLSCHEMA.DELETESCHEMA(schemaurl     => p_SchemaURL
							   --,delete_option => DBMS_XMLSCHEMA.DELETE_RESTRICT
							   );
	-- end pl/sql
exception when others all then
	null;
end;


-- инициализация маленького XSD из файла
procedure initXSDFromFile(p_xsd out clob, p_SchemaUrl in varchar2(100)) is
begin
	dbms_lob.createTemporary(p_xsd, false);
	::[RUNTIME].[LOB_UTILS].file2clob(p_clob       == p_xsd
	   						         ,p_filedir    == './tmp'
							         ,p_filename   == 'test.xsd');
							
	p_xsd := convert(p_xsd, 'CL8ISO8859P5', 'CL8MSWIN1251');
	
	--dbms_xmlschema.registerschema(schemaurl == p_name, schemadoc == xsdClob);	
	--dbms_lob.freeTemporary(xsdClob);
end;


-- инициализация маленького XML из файла
procedure initXMLFromFile(p_xml out clob) is
begin
	dbms_lob.createTemporary(p_xml, false);
	::[RUNTIME].[LOB_UTILS].file2clob(p_clob       == p_xml
	   						         ,p_filedir    == './tmp'
							         ,p_filename   == 'test.xml');
							
	p_xml := convert(p_xml, 'CL8ISO8859P5', 'CL8MSWIN1251');
end;


